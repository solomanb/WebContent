<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>ImpactWeb</display-name>
	<servlet>
		<description>
		</description>
		<display-name>StockServlet</display-name>
		<servlet-name>StockServlet</servlet-name>
		<servlet-class>com.demo.StockServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>WorkingServlet</display-name>
		<servlet-name>WorkingServlet</servlet-name>
		<servlet-class>com.demo.WorkingServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>StockAudioServlet</display-name>
		<servlet-name>StockAudioServlet</servlet-name>
		<servlet-class>com.demo.StockAudioServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>StockServletTwitter</display-name>
		<servlet-name>StockServletTwitter</servlet-name>
		<servlet-class>com.demo.StockServletTwitter</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StockServlet</servlet-name>
		<url-pattern>/StockServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WorkingServlet</servlet-name>
		<url-pattern>/WorkingServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StockAudioServlet</servlet-name>
		<url-pattern>/StockAudioServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>StockServletTwitter</servlet-name>
		<url-pattern>/StockServletTwitter</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
